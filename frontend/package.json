    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet-async": "^1.3.0",
    "react-map-gl": "^7.1.3",
    "react-redux": "^8.1.3",
    "react-router-dom": "^6.15.0",
    "react-toastify": "^9.1.3",
    "styled-components": "^6.0.8",
